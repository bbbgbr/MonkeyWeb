block script
    script(src="js/adminHybridInfo.min.js")
block body
    .container
        h1#fhbSubj FHB:#
        .row
            p.col-auto#day Day:Monday
            p.col-auto#time 8:00-10:00
        .row
            p.col-auto#hbID sadasdaskdkaskdkljakld
        .table-responsive
            table.table.table-hover
                thead.thead-dark
                    tr
