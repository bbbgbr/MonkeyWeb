extends devTemplate
block content
	.block-header
		h2 QUOTA
	.row.clearfix
		.col-lg-12.col-md-12.col-sm-12.col-xs-12
			.card
				.header
					h2
						| QUOTA DEMO
						small Enter student id and subject to add quota
				.body
					form
						.form-group.form-float
							.form-line
								input#studentID.form-control(type='text')
								label.form-label Student ID
							br
							.form-line
								input#subject.form-control(type='text')
								label.form-label Subject
							br
							.form-line
								input#quota.form-control(type='text')
								label.form-label Quota
						button.btn.btn-primary.m-t-15.waves-effect(type='button', onclick='addQuota();') ADD
block script
		script(src='dev/js/quota.js')